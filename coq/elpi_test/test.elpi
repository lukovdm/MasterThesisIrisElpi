type sep_split list string -> open-tactic.
  % sep_split HS' G GL :- refine {{ tac_sep_split _ _ false lp:HS' _ _ _ }} G GL.

  inamed.aux I O :- O = {{ INamed lp:I }}.

  solve (goal _Ctx _Trigger _Type _Proof [str HSS] as _G) _GS :-
    rex.split " " HSS HS,
    std.map HS inamed.aux HI,
    coq.say "Args:" HI.
    % coq.say {{ tac_sep_split _ _ false lp:HS _ _ _ }}.